{
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint \"app/javascript/src/**/*.{js,jsx}\"",
    "lint:fix": "eslint --fix \"app/javascript/src/**/*.{js,jsx}\"",
    "dev": "bundle exec vite dev",
    "build": "bundle exec vite build",
    "clear:cache": "bundle exec vite clobber"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jam3": "^0.2.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.35",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "tailwindcss": "^3.4.1",
    "vite": "^5.0.10",
    "vite-plugin-ruby": "^5.0.0"
  },
  "dependencies": {
    "@bigbinary/neeto-cist": "^1.0.8",
    "@tanstack/react-query": "^5.24.1",
    "@vitejs/plugin-react": "^4.2.1",
    "antd": "^5.16.1",
    "antd-input-otp": "^2.1.0",
    "antd-phone-input": "^0.3.5",
    "axios": "^1.6.4",
    "classnames": "^2.5.1",
    "i18next": "^23.10.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "qs": "^6.12.1",
    "ramda": "^0.29.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.21.1"
  },
  "lint-staged": {
    "app/**/*.{js,jsx,json}": [
      "prettier --write",
      "eslint --fix"
    ],
    "{package.json,.eslintrc.js,.prettierrc.js}": [
      "prettier --write",
      "eslint --fix"
    ],
    ".eslint-rules/**/*": [
      "prettier --write",
      "eslint --fix"
    ],
    "./**/*.rb": [
      "bundle exec rubocop -a --force-exclusion"
    ],
    "{Gemfile,Rakefile,config.ru}": [
      "bundle exec rubocop -a --force-exclusion"
    ],
    "app/views/**/*.html.erb": [
      "bundle exec erblint --lint-all --format compact -a"
    ]
  }
}
